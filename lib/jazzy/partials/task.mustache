<div class="task-group">
  <div class="task-name-container">
    <a name="//jazzy_ref/doc/uid/{{uid}}"></a>
    <a href="#//jazzy_ref/doc/uid/{{uid}}">
      <h3 class="section-name">{{name}}</h3>
    </a>
  </div>
  <ul class="task-group-list">
    <li class="item symbol">
      <div class="task-group-term">
        <code>
        <a name="//jazzy_ref/occ/instm/{{name}}/initWithString:"></a><a name="//jazzy_ref/doc/uid/UID1-initWithString_"></a><a name="//jazzy_ref/swift/instctr/{{name}}/string(String)"></a>
        <a class="x-instance-method Swift" href="#//jazzy_ref/occ/instm/{{name}}/initWithString:" title="Returns an {{name}} object initialized with the characters of a given string and no attribute information.">init(string:)</a>
        </code>
      </div>
      <div class="height-container">
        <div class="pointy-thing-container"></div>
        <section class="section instance-method">
          <div class="pointy-thing"></div>
          <div class="abstract">
            {{{abstract}}}
          </div>
          <div class="declaration">
            <h4>Declaration</h4>
            
            <div class="Swift">
              <p class="aside-title">Swift</p>
              <p class="para">
              <code>{{declaration}}</code>
              </p>
            </div>
          </div>
          {{#parameters.length}}
          <div class="parameters">
            <h4>Parameters</h4>
            <table class="graybox" border="0" cellspacing="0" cellpadding="5">
              <tbody>
                {{#parameters}}
                {{> jazzy/partials/parameter}}
                {{/parameters}}
              </tbody>
            </table>
          </div>
          {{/parameters.length}}
          <div class="result-description">
            <h4>Return Value</h4>
            {{{return}}}
          </div>
        </section>
      </div>
    </li>
  </ul>
</div>
